[{"@id":"_:genid1","@type":["http://www.w3.org/2002/07/owl#Restriction"],"http://www.w3.org/2002/07/owl#onProperty":[{"@id":"https://w3id.org/ontocis/composition#partOf"}],"http://www.w3.org/2002/07/owl#someValuesFrom":[{"@id":"https://w3id.org/ontocis/competences#Category"}]},{"@id":"_:genid2","@type":["http://www.w3.org/2002/07/owl#Restriction"],"http://www.w3.org/2002/07/owl#onProperty":[{"@id":"https://w3id.org/ontocis/composition#partOf"}],"http://www.w3.org/2002/07/owl#someValuesFrom":[{"@id":"https://w3id.org/ontocis/competences#Competence"}]},{"@id":"https://w3id.org/ontocis/competences","@type":["http://www.w3.org/2002/07/owl#Ontology"],"http://purl.org/dc/terms/#license":[{"@id":"https://opensource.org/licenses/MIT"}],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"es","@value":"Describe un conjunto de competencias, frases que describen de manera general el conocimiento y capacidades que debe adquirir un estudiante durante una asignatura o programa, y categorias bajo las cuales organizarlas."},{"@language":"en","@value":"Describes a set of competences, general statementes detailing the desired knowledge and skills of student graduating from a course or program should adquire, and categories under which organize them."}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"en","@value":"OntoCIS Competences"},{"@language":"es","@value":"OntoCIS Competencias"}],"http://www.w3.org/2002/07/owl#imports":[{"@id":"https://w3id.org/ontocis/composition"}]},{"@id":"https://w3id.org/ontocis/competences#Category","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"en","@value":"Categories under which the competences are organized"},{"@language":"es","@value":"Categorías bajo las cuales se organizan las competencias"}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"en","@value":"Category"},{"@language":"es","@value":"Categoría"}]},{"@id":"https://w3id.org/ontocis/competences#Competence","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"en","@value":"A general statement detailing the desired knowledge and skills of student graduating from our course or program."},{"@language":"es","@value":"Una frase que describe de manera general el conocimiento y capacidades que debe adquirir un estudiante."}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"es","@value":"Competencia"},{"@language":"en","@value":"Competency"}],"http://www.w3.org/2002/07/owl#equivalentClass":[{"@id":"_:genid1"}]},{"@id":"https://w3id.org/ontocis/competences#Learning_Outcome","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"en","@value":"A very specific statement that describes exactly what a student will be able to do in some measurable way"},{"@language":"es","@value":"Una frase que describe exactamente lo que un estudiante podrá hacer de manera mensurable/evaluable."}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"en","@value":"Learning Outcome"},{"@language":"es","@value":"Objetivo de Aprendizaje"}],"http://www.w3.org/2002/07/owl#equivalentClass":[{"@id":"_:genid2"}]},{"@id":"https://w3id.org/ontocis/competences#contributesTo","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"es","@value":"Indica el grado de contribución al desarrollo de una competencia"},{"@language":"en","@value":"It states the degree of contribution to the development of a competency"}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"en","@value":"contributes to"},{"@language":"es","@value":"contribuye a"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"https://w3id.org/ontocis/competences#Competence"}]},{"@id":"https://w3id.org/ontocis/competences#develops","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"es","@value":"Indica que la competencia es total o parcialmente desarrollada a lo largo de una asignatura, es evaluada en la asignatura y se cuenta con evidencia de la evaluación."},{"@language":"en","@value":"It states that the competency is totally or partially developed in a course, it is assessed and there is evidence of the assessment."}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"es","@value":"desarrolla"},{"@language":"en","@value":"develops"}],"http://www.w3.org/2000/01/rdf-schema#subPropertyOf":[{"@id":"https://w3id.org/ontocis/competences#contributesTo"}]},{"@id":"https://w3id.org/ontocis/competences#hasLearningOutcome","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"es","@value":"Indica que objetivos de aprendizaje tiene una cosa."},{"@language":"en","@value":"It states the learning outcomes for a certain thing."}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"en","@value":"has learning outcome"},{"@language":"es","@value":"tiene como objetivo de aprendizaje"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"https://w3id.org/ontocis/competences#Learning_Outcome"}],"http://www.w3.org/2000/01/rdf-schema#subPropertyOf":[{"@id":"http://www.w3.org/2002/07/owl#topObjectProperty"}]},{"@id":"https://w3id.org/ontocis/competences#partiallyDevelops","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@language":"es","@value":"Indica que la asignatura contribuye de alguna manera al desarrollo de la competencia, pero no esta no es evaluada."},{"@language":"en","@value":"It states that the course contributes to the development of the competency but it is not assessed."}],"http://www.w3.org/2000/01/rdf-schema#label":[{"@language":"es","@value":"desarrolla parcialmente"},{"@language":"en","@value":"partially develops"}],"http://www.w3.org/2000/01/rdf-schema#subPropertyOf":[{"@id":"https://w3id.org/ontocis/competences#contributesTo"}]}]